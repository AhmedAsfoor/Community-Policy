{
  "OutboundType": {
    "type": "String",
    "metadata": {
      "description": "The outbound (egress) routing method.",
      "displayName": "Outbound Type"
    },
    "allowedValues": [
      "loadBalancer",
      "userDefinedRouting"
    ],
    "defaultValue": "userDefinedRouting"
  },
  "effect": {
    "type": "String",
    "metadata": {
      "description": "Enable or disable the execution of the policy",
      "displayName": "Effect"
    },
    "allowedValues": [
      "Audit",
      "Deny",
      "Disabled"
    ],
    "defaultValue": "Deny"
  }
}
